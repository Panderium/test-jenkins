{{- $projectname := .ProjectName -}}
GITLAB=10.1.38.31
GIT_USER=root
GIT_PWD=solutec2018
REPOSITORY_PROD=pull.nexus.prod.lab.solutec
REPOSITORY_PREPROD=pull.nexus.preprod.lab.solutec
DNS=lab.solutec
DOCKERFILE=Dockerfile
{{range .Services -}}
    {{- if eq .Name "BDD" -}}
        {{- range .Values}}
{{.}}_IMAGE={{.}}:cert
{{.}}_PREPROD=preprod-databases
{{.}}_PROD=prod-databases

{{retrieveEnvVariable .}}
        {{end -}}
    {{else}}
{{$projectname}}_{{.Name}}_IMAGE={{$projectname}}_{{.Name}}:latest
{{$projectname}}_{{.Name}}_PREPROD=preprod-services
{{$projectname}}_{{.Name}}_PROD=prod-services
        {{- if eq .Name "back"}}
{{$projectname}}_{{.Name}}_URL={{$projectname}}.api
        {{- else}}
{{$projectname}}_{{.Name}}_URL={{$projectname}}
        {{- end}}
{{$projectname}}_{{.Name}}_PORT=####
    {{- end -}}
{{- end -}}