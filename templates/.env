GITLAB=10.1.38.31
GIT_USER=root
GIT_PWD=solutec2018
REPOSITORY=pull.nexus.prod.lab.solutec
DNS=lab.solutec
{{range .Services -}}
    {{- if eq .Name "bdd" -}}
        {{- range .Values}}
{{.}}_IMAGE={{.}}:cert
{{.}}_PREPROD=preprod-databases
